#include "ultrasonic.h"
#include "lcd.h"

int main() {
	LCD_init();
	Ultrasonic_init();
	LCD_displayString("Distance = ");
	sei();


	while (1) {
			Ultrasonic_readDistance();
			//LCD_moveCursor(0, 12);
			//LCD_intgerToString(Ultrasonic_readDistance());
	}
}
